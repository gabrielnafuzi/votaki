    "eslint-plugin-react": "^7.31.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "storybook-dark-mode": "^1.1.0",
